{% extends 'base-back.html.twig' %}
{% form_theme form 'bootstrap_4_layout.html.twig' %}

{% block title %}New Produits{% endblock %}

{% block body %}
    <h1>Create new Produits</h1>
    {{ form_start(form, {attr: {'novalidate': 'novalidate'}}) }}
    <table>
        <tr>
            <td>{{ form_label(form.prix) }}</td>
            <td>{{ form_widget(form.prix) }}</td>
            <td>{{ form_errors(form.prix) }}</td>
        </tr>
        <tr>
            <td>{{ form_label(form.nom) }}</td>
            <td>{{ form_widget(form.nom) }}</td>
            <td>{{ form_errors(form.nom) }}</td>
        </tr>
        <td> {{ form_label(form.description) }}</td>
        <td>{{ form_widget(form.description) }}</td>
        <td>{{ form_errors(form.description) }}</td>
    </table>
    {{ include('produits/_form.html.twig') }}
    {{ form_end(form) }}



    <a href="{{ path('app_produits_index') }}">back to list</a>
{% endblock %}
