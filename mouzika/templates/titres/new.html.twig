{% extends 'base-back.html.twig' %}
{% form_theme form 'bootstrap_4_layout.html.twig' %}

{% block title %}New Titres{% endblock %}

{% block body %}
    <h1>Create new Titres</h1>
    {{ form_start(form, {attr: {'novalidate': 'novalidate'}}) }}
<table>
    <tr>
        <td>{{ form_label(form.nom) }}</td>
        <td>{{ form_widget(form.nom) }}</td>
        <td>{{ form_errors(form.nom) }}</td>
    </tr>
    <tr>
        <td>{{ form_label(form.artiste) }}</td>
        <td>{{ form_widget(form.artiste) }}</td>
        <td>{{ form_errors(form.artiste) }}</td>
    </tr>
</table>


    {{ include('titres/_form.html.twig') }}
    {{ form_end(form) }}

    <a href="{{ path('app_titres_index') }}">back to list</a>
{% endblock %}
